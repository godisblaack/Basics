{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffe9aa",
          "powerline_symbol": "\u279C",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#95ffa4",
          "powerline_symbol": "\u279C",
          "style": "powerline",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#00ffff",
          "powerline_symbol": "\u279C",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \u279C ",
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#95ffa4",
          "powerline_symbol": "\u279C",
          "style": "powerline",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "foreground": "#906cff",
          "powerline_symbol": "\u279C",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "foreground": "#ff8080",
          "powerline_symbol": "\u279C",
          "style": "powerline",
          "template": " \ue20f ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#95ffa4",
          "style": "plain",
          "template": "\u279c ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
